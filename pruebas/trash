    function crearfichanegre(){
      var fn = document.createElement("img");
      fn.src = "fichanegre.png";
      fn.setAttribute("style","position:relative");
      fn.setAttribute("width","50");
      fn.setAttribute("height","50");
      fn.style.left = 3+ml*3;
      fn.style.top = 2+mt*4;
      var div = document.getElementById("x");
      div.appendChild(fn);

      div.setAttribute("style","position:absolute");
    }
    function crearfichablanca(){
      var fb = document.createElement("img");
      fb.src = "fichablanca.png";
      fb.setAttribute("style","position:relative");
      fb.setAttribute("width","50");
      fb.setAttribute("height","50");
      fb.style.left = -47+ml*4;
      fb.style.top = 2+mt*4;
      var div = document.getElementById("x");
      div.appendChild(fb);
      div.setAttribute("style","position:absolute");
    }
